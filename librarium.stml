  <liber numerus="12183" status="enriched">
    <titulus>Wau-Bun, The Early Day in the Northwest</titulus>
    <auctor>Juliette Augusta Magill Kinzie (Mrs. John H. Kinzie)</auctor>
    <annus>1856</annus>
    <tags>
      <tag>non-fiction</tag>
